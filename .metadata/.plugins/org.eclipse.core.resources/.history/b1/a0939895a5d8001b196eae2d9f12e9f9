package com.group.mypage.dto;

import java.util.Date;

import com.group.employee.dto.Department;
import com.group.employee.dto.Job;
import com.group.employee.dto.Leave;
import com.group.employee.dto.Position;

public class Employee {
	public String employee_id;
	public String name;
	
	public Department department_id;
	public Job job_id;
	public Position position_id;
	
	public String phone_number;
	public String email;
	public Date hire_date;
	public char employee_status;
	public String password;
	
	//사원은 휴가를 가질 수 있다
	public Leave leave;
}
